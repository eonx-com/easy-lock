{
    "name": "eonx-com/easy-lock",
    "description": "Framework agnostic locking features",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "symfony/lock": "^4.2 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5",
        "symfony/symfony": "^4.2 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyLock\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyLock\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.6-dev"
        }
    }
}
